# Backend .env.example
# Copy this file to .env and fill in the values

# AI Providers (optional depending on features you use)
OPENAI_API_KEY=
GROQ_API_KEY=

# Supabase (required)
SUPABASE_URL=
SUPABASE_SERVICE_ROLE_KEY=
# JWKS URL usually looks like: https://<project-ref>.supabase.co/auth/v1/.well-known/jwks.json
SUPABASE_JWKS_URL=

# Server
PORT=6000
# Origin of your Next.js app
FRONTEND_ORIGIN=http://localhost:3000
